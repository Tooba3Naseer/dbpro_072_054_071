								@Html.DropDownList("Status",new SelectList(Enum.GetValues(typeof
							   (Status))),"", new { @class = "form-control" })

							</td>
						</tr>



						<tr>
							<td><input type="submit" value="Update" class="btn btn-default" /></td>
						<tr>
					</table>
				
						@Html.ActionLink("Back to List", "Index")
					</div>

				</div>
			</div>
		
