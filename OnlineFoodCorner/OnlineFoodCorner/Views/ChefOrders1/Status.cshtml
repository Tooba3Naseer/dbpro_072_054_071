				<div class="table-responsive">
					<table class="table">
						<tr>
							<td> <label for="fname"><i class="fa fa-user"></i> Order ID</label> </td>
							<td>
								@Html.DropDownList("OrderId", null, htmlAttributes: new { @class = "form-control" })
								@Html.ValidationMessageFor(model => model.OrderId, "", new { @class = "text-danger" })
							</td>
						</tr>
						<tr>
							<td> <label for="fname"><i class="fa fa-user"></i> Chef ID</label> </td>
							<td>
								@Html.DropDownList("ChefId", null, htmlAttributes: new { @class = "form-control" })
								@Html.ValidationMessageFor(model => model.ChefId, "", new { @class = "text-danger" })
							</td>
						</tr>

						<tr>
						<td> <label for="fname"><i class="fa fa-user"></i>Assigned Date</label> </td>
						<td>
							@Html.EditorFor(model => model.AssignmentDate, new { htmlAttributes = new { @class = "form-control" } })
