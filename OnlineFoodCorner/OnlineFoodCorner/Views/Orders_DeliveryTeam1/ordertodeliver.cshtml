<body>
	<!-- Preloader Starts -->
	<div class="preloader">
		<div class="spinner"></div>
	</div>
	<!-- Preloader End -->
	<!-- Header Area Starts -->
	<header class="header-area header-area2">
		<div class="container">
			<div class="row">
				<div class="col-lg-2">
					<div class="logo-area">
						<a href="index.html"><img src="~/assets/images/logo/l1.png" alt="logo"></a>
					</div>
				</div>
				<div class="col-lg-10">
					<div class="custom-navbar">
						<span></span>
						<span></span>
						<span></span>
					</div>
					<div class="main-menu main-menu2">
						<ul>
							<li class="active"><a href="@Url.Action("DelIndex","Home")">Home</a></li>




							<li><a href="@Url.Action("ordertodeliver","Orders_DeliveryTeam1")">Order Details</a></li>

							<li>
								<a href="@Url.Action("MyDelProfile","Users")">My Profile</a>

							</li>

							<li>
								<a href="@Url.Action("deliveredorder","Orders_DeliveryTeam1")">Delivered Orders</a>

							</li>

							<li>
								<a href="@Url.Action("Index","Home")">LogOut</a>

							</li>
							<li><a href="#AboutUs">About</a></li>

						</ul>
					</div>
				</div>
			</div>
		</div>
	</header>
	<!-- Header Area End -->
	<!-- Banner Area Starts -->
	<section class="banner-area banner-area2 blog-page text-center">
		<div class="container">
			<div class="row">
				<div class="col-lg-12">
					<h1><i>Order Delivery Details</i></h1>

				</div>
			</div>
		</div>
	</section>
	<!-- Banner Area End -->
	<!--================Cart Area =================-->

	<section class="cart_area">
		<div class="container">
			<div class="cart_inner">
				<div class="table-responsive">
					<table class="table">

						<tr>
							<th>
								@Html.DisplayNameFor(model => model.Delivery_Team.Name)
							</th>
							<th></th>
							<th>
								@Html.DisplayNameFor(model => model.Order.EmployeeId)

							</th>
							<th></th>
							<th>
								@Html.DisplayNameFor(model => model.Order.CustomerId)
							</th>
							<th></th>
							<th>
								@Html.DisplayNameFor(model => model.OrderId)
							</th>
							<th></th>
							<th>
								@Html.DisplayNameFor(model => model.AssignmentDate)
							</th>
							<th></th>
							<th>
								@Html.DisplayNameFor(model => model.Order.DeliveryAddress)
							</th>
							<th></th>
							<th>
								@Html.DisplayNameFor(model => model.Order.DeliveryArea)
							</th>
							<th></th>
							<th>
								@Html.DisplayNameFor(model => model.Order.DeliveryCity)
							</th>
							<th></th>

							<th></th>
						</tr>

						@foreach (var item in Model)
						{
							<tr>
								<td>
									@Html.DisplayFor(modelItem => item.Delivery_Team.Name)
								</td>
								<td></td>
								<td>
									@Html.DisplayFor(modelItem => item.Order.EmployeeId)
								</td>
								<td></td>
								<td>
									@Html.DisplayFor(modelItem => item.Order.CustomerId)
								</td>
								<td></td>
								<td>
									@Html.DisplayFor(modelItem => item.OrderId)
								</td>
								<td></td>
								<td>
									@Html.DisplayFor(modelItem => item.AssignmentDate)
								</td>
								<td></td>

								<td>
									@Html.DisplayFor(modelItem => item.Order.DeliveryAddress)
								</td>
								<td></td>
								<td>
									@Html.DisplayFor(modelItem => item.Order.DeliveryArea)
								</td>
								<td></td>
								<td>
									@Html.DisplayFor(modelItem => item.Order.DeliveryCity)
								</td>

								<td>
									@Html.ActionLink("Update Status", "UpdateStatus", new { id = item.Order.OrderId }) |

								</td>
							</tr>
						}

					</table>


				</div>
			</div>
		</div>
	</section>
